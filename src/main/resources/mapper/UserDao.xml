<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lowen.bus.dao.UserDao">
	
	<select id="queryUserByPage" parameterType="page" resultType="com.lowen.bus.bean.UserInfo">
		SELECT
			t.id,
			t.name,
			t.age
		FROM
			t_user t
	</select>
	
</mapper>
          